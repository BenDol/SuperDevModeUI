<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
             ui:generateKeys='com.google.gwt.i18n.server.keygen.MD5KeyGenerator'
             ui:generateLocales='default'>

    <ui:with field="res" type="nz.co.doltech.gwt.sdm.ui.SuperDevModeUI.Resources"/>

    <g:HTMLPanel ui:field="sdmPanel" addStyleNames="{res.style.sdm}">
        <g:Button ui:field="btnCompile" text="Compile" icon="COG" addStyleNames="{res.style.compileBtn}"/>
    </g:HTMLPanel>
</ui:UiBinder>
